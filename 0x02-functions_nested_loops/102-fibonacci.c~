#include<stdio.h>

/**
 * main - Entry point of the program
 * Return : Always 0 (success/correct)
 */

int main(void)
{
unsigned long s1;
unsigned long s;
unsigned long i;
unsigned long tp;
s = 2;
s1 =1;

for (i = 0; i <= 50; i++)
{
if (i == 0)
{
printf("%i", s1);
printf(",");
printf(" ");
printf("%i", s);
}
else if (i < 50)
{
tp = s;
printf(",");
printf(" ");
s = s+s1;
printf("%i", s);
s1 = tp;
}
else
{
printf("\n");
}
}
return (0);
}
